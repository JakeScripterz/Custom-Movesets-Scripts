spawn(function()
wait(1)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")

local ball = Instance.new("Part")
ball.Shape = Enum.PartType.Ball
ball.Size = Vector3.new(1000, 1000, 1000)
ball.Color = Color3.new(0, 0, 0)
ball.Material = Enum.Material.Neon
ball.Transparency = 0
ball.Anchored = true
ball.CanCollide = false
ball.Parent = game.Workspace
ball.Position = rootPart.Position

local lighting = game:GetService("Lighting")
local oldAmbient = lighting.Ambient
local oldBrightness = lighting.Brightness
local oldtime = lighting.TimeOfDay
lighting.Ambient = Color3.new(0.4, 0.35, 0.35)
lighting.Brightness = 0.3
lighting.TimeOfDay = "00:00:00"
local disappearTime = 4.5

task.delay(disappearTime, function()
if ball then
ball:Destroy()
end
lighting.Ambient = oldAmbient
lighting.Brightness = oldBrightness 
lighting.TimeOfDay = oldtime end)
end)

spawn(function()
local sp = 1.5 --speed it coming up and down
local length = 4.5 --how long it last before go down
local hrp = game.Players.LocalPlayer.Character["HumanoidRootPart"]
local domain = game:GetObjects("rbxassetid://74862316865526")[1]
domain.Parent = workspace
local shrine = domain["ReplicatedStorage"].Shrine
shrine:SetPrimaryPartCFrame(hrp.CFrame * CFrame.new(0,-30,30))
local ts = game:GetService("TweenService")
local inf = TweenInfo.new(sp, Enum.EasingStyle.Linear, Enum.EasingDirection.In, 0, false, 0)
for _, part in ipairs(shrine:GetDescendants()) do
if part:IsA("MeshPart") then
spawn(function()
local g = { CFrame = part.CFrame * CFrame.new(0,43,0) }
local up = ts:Create(part, inf, g)
up:Play() 
wait(length)
local g2 = { CFrame = part.CFrame * CFrame.new(0,-43,0) }
local down = ts:Create(part, inf, g2)
down:Play() 
down.Completed:Connect(function()
domain:Destroy()
end)
end)
end end
end)
spawn(function()
wait(1.4)
local ModelId = 85910295444347

local MoveRight = 0
local MoveLeft = 0
local MoveUp = 0
local MoveDown = 0
local MoveBack = 0
local MoveForward = 7

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:FindFirstChild("HumanoidRootPart")

if not rootPart then
    warn("HumanoidRootPart not found!")
    return
end

local d = game:GetObjects("rbxassetid://" .. ModelId)[1]

if d:IsA("Part") then
    d.Parent = character
    d.CFrame = rootPart.CFrame * CFrame.new(0, 0, -MoveForward)
    d.CanCollide = false
    d.Anchored = false

    -- Create and apply a WeldConstraint
    local weld = Instance.new("WeldConstraint")
    weld.Part0 = rootPart
    weld.Part1 = d
    weld.Parent = d

elseif d:IsA("Model") then
    if not d.PrimaryPart then
        local p = Instance.new("Part")
        p.Size = Vector3.new(1, 1, 1)
        p.Transparency = 1
        p.Anchored = false
        p.CanCollide = false
        p.Parent = d
        d.PrimaryPart = p
    end

    d.Parent = character
    d:SetPrimaryPartCFrame(rootPart.CFrame * CFrame.new(MoveRight - MoveLeft, MoveUp - MoveDown, -MoveForward))

    for _, part in ipairs(d:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = false
            part.Anchored = false

            local weld = Instance.new("WeldConstraint")
            weld.Part0 = rootPart
            weld.Part1 = part
            weld.Parent = part
        end
    end
end

task.delay(2.5, function()
    if d then
        d:Destroy()
    end
end)
end)



local Sound = 6590147536
local Volume = 4
local Speed = 1

s= Instance.new("Sound", game.Players.LocalPlayer.Character.HumanoidRootPart)
s.SoundId = "rbxassetid://" .. Sound
s:Play()
s.Volume = Volume
s.PlaybackSpeed = Speed

task.delay(2, function()
local Sound = 7817336081
local Volume = 2
local Speed = 1

s= Instance.new("Sound", game.Players.LocalPlayer.Character.HumanoidRootPart)
s.SoundId = "rbxassetid://" .. Sound
s:Play()
s.Volume = Volume
s.PlaybackSpeed = Speed
end)

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local soundToBlock = 14762034452

local function onSoundPlayed(sound)
    if sound.SoundId == "rbxassetid://" .. soundToBlock then
        sound.Volume = 0
    end
end

local function blockSpecificSound()
    workspace.DescendantAdded:Connect(function(descendant)
        if descendant:IsA("Sound") then
            descendant.Played:Connect(function() onSoundPlayed(descendant) end)
        end
    end)

    for _, sound in ipairs(workspace:GetDescendants()) do
        if sound:IsA("Sound") then
            sound.Played:Connect(function() onSoundPlayed(sound) end)
        end
    end
end

blockSpecificSound()

humanoid.Died:Connect(function()
    for _, sound in ipairs(workspace:GetDescendants()) do
        if sound:IsA("Sound") and sound.SoundId == soundToBlock then
            sound.Volume = 1
        end
    end
end)

    end

end
