local animationId = 10466974800


local player = game.Players.LocalPlayer

local character = player.Character or player.CharacterAdded:Wait()

local humanoid = character:WaitForChild("Humanoid")


local function onAnimationPlayed(animationTrack)

    if animationTrack.Animation.AnimationId == "rbxassetid://" .. animationId then


local p = game.Players.LocalPlayer

local Humanoid = p.Character:WaitForChild("Humanoid")


for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do

    animTrack:Stop()

local L = game.ReplicatedStorage.Emotes.redEnergy
local CEClones = {}
local Clones = 1

local rightArm = game.Players.LocalPlayer.Character:WaitForChild("Right Arm")
local leftArm = game.Players.LocalPlayer.Character:WaitForChild("Left Arm")

local function D()
    for _, clone in ipairs(CEClones) do
        if clone and clone.Parent then
            clone:Destroy()
        end
    end
    CEClones = {}
end

for i = 1, Clones do

    local CERight = L:Clone()  
    CERight.Parent = rightArm
    CERight.CFrame = rightArm.CFrame * CFrame.new(0, -0.8, 0)
    CERight.Anchored = false

    local weldRight = Instance.new("WeldConstraint")
    weldRight.Parent = CERight
    weldRight.Part0 = rightArm
    weldRight.Part1 = CERight

    table.insert(CEClones, CERight)

    for _, child in ipairs(CERight:GetChildren()) do    
        if child:IsA("ParticleEmitter") then
            child:Emit(1)
            child.Enabled = true
        end
    end

    local CELeft = L:Clone()  
    CELeft.Parent = leftArm
    CELeft.CFrame = leftArm.CFrame * CFrame.new(0, -0.8, 0)
    CELeft.Anchored = false

    local weldLeft = Instance.new("WeldConstraint")
    weldLeft.Parent = CELeft
    weldLeft.Part0 = leftArm
    weldLeft.Part1 = CELeft

    table.insert(CEClones, CELeft)

    for _, child in ipairs(CELeft:GetChildren()) do    
        if child:IsA("ParticleEmitter") then
            child:Emit(1)
            child.Enabled = true
        end
    end
end

coroutine.wrap(function()
task.wait(1.4)
D()
end)()
brrgsfx.Parent = workspace
brrgsfx.Volume = 5
brrgsfx.TimePosition = 0.4
brrgsfx:Play()

local ModelId = 84820055642325
local MoveRight = 0
local MoveLeft = 0
local MoveUp = 0
local MoveDown = 0
local MoveBack = 0
local MoveForward = 6 -- Adjust this value to move it further in front

-- Load the model
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:FindFirstChild("HumanoidRootPart")

if not rootPart then
    warn("HumanoidRootPart not found!")
    return
end

local d = game:GetObjects("rbxassetid://" .. ModelId)[1]

if d:IsA("Part") then
    d.Parent = character
    d.CFrame = rootPart.CFrame * CFrame.new(0, 0, -MoveForward) -- Move in front
    d.CanCollide = false -- Make it non-collidable
    d.Anchored = false -- Disable anchoring

    -- Create and apply a WeldConstraint
    local weld = Instance.new("WeldConstraint")
    weld.Part0 = rootPart
    weld.Part1 = d
    weld.Parent = d

elseif d:IsA("Model") then
    if not d.PrimaryPart then
        local p = Instance.new("Part")
        p.Size = Vector3.new(100, 100, 100)
        p.Transparency = 10
        p.Anchored = false
        p.CanCollide = false
        p.Parent = d
        d.PrimaryPart = p
    end

    d.Parent = character
    d:SetPrimaryPartCFrame(rootPart.CFrame * CFrame.new(MoveRight - MoveLeft, MoveUp - MoveDown, -MoveForward))

    -- Set non-collidable and disable anchoring for all parts
    for _, part in ipairs(d:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = false
            part.Anchored = false

            -- Create and apply a WeldConstraint
            local weld = Instance.new("WeldConstraint")
            weld.Part0 = rootPart
            weld.Part1 = part
            weld.Parent = part
        end
    end
end

-- Destroy the model after 1 second
task.delay(1.4, function()
    if d then
        d:Destroy()
    end
end)




end


local AnimAnim = Instance.new("Animation")

AnimAnim.AnimationId = "rbxassetid://13560306510"

local Anim = Humanoid:LoadAnimation(AnimAnim)


local startTime = 0


Anim:Play()

Anim:AdjustSpeed(1)

Anim.TimePosition = startTime

Anim:AdjustSpeed(3)

local b = game.Players.LocalPlayer.Character:WaitForChild("BarrageBind")
b:Destroy()


    end

end

