local function trail(parent, color1, color2, color3, light)
local s = game.ReplicatedStorage.Resources.VanishingKick["Trail2"]:Clone()
s.Parent = workspace
s.CFrame = parent.CFrame

for _, child in ipairs(s:GetChildren()) do
if child:IsA("Trail") and child.Color then
child.LightEmission = light
child.Color =ColorSequence.new(Color3.new(color1, color2, color3))
end
end

local skibidi = game:GetService("RunService").Heartbeat:Connect(function()
s.CFrame = parent.CFrame
end)
game.Players.LocalPlayer.CharacterAdded:Connect(function()
skibidi:Disconnect()
s:Destroy()
end)
spawn(function()
wait(1) s:Destroy()
skibidi:Disconnect()
end)

end
spawn(function()
for i=1,5 do
trail(game.Players.LocalPlayer.Character["Right Leg"], 1, 0.1, 0.1, 1)
wait() end end)
wait(.1)

