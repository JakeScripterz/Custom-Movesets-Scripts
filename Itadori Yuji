w.CFrame = w.CFrame * CFrame.new(0,0,-9) * CFrame.Angles(math.rad(90),0,0)
for _, child in ipairs(w:GetChildren()) do
if child:IsA("ParticleEmitter") and child.Name == "2" or child.Name == "Fire2" then
child.Color = ColorSequence.new(Color3.new(1,0,0))
child:Emit(0)
child.Enabled = false
else
child.Color = ColorSequence.new(Color3.new(1,.4,.4))
child:Emit(3)
child.Enabled = false
end
end
local w = f["YoneExplosion"].Explosion:Clone()
w.Parent = hrp
w.CFrame = w.CFrame * CFrame.new(0,0,-9) * CFrame.Angles(math.rad(90),0,0)
for _, child in ipairs(w:GetChildren()) do
if child:IsA("ParticleEmitter") then
child.Color = ColorSequence.new(Color3.new(0,0,0))
child:Emit(5)
child.Enabled = false
end
end
local w = f["YoneExplosion"].Explosion:Clone()
w.Parent = hrp
w.CFrame = w.CFrame * CFrame.new(0,0,-9) * CFrame.Angles(math.rad(90),0,0)
for _, child in ipairs(w:GetChildren()) do
if child:IsA("ParticleEmitter") and child.Name == "2" or child.Name == "Fire2" then
child.Color = ColorSequence.new(Color3.new(1,0,0))
child:Emit(0)
child.Enabled = false
else
child.Color = ColorSequence.new(Color3.new(1,.4,.4))
child:Emit(2)
child.Enabled = false
end
end
local w = f["YoneExplosion"].Explosion:Clone()
w.Parent = hrp
w.CFrame = w.CFrame * CFrame.new(0,0,-9) * CFrame.Angles(math.rad(90),0,0)
for _, child in ipairs(w:GetChildren()) do
if child:IsA("ParticleEmitter") then
child.Color = ColorSequence.new(Color3.new(0,0,0))
child:Emit(3)
child.Enabled = false
end
end
local w = f["YoneExplosion"].Dust:Clone()
w.Parent = hrp
for _, child in ipairs(w:GetChildren()) do
if child:IsA("ParticleEmitter") then
child:Emit(5)
child.Enabled = false
end
end

f:Destroy()



    end

end

